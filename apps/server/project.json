{
  "name": "server",
  "importPath": "@marai/server",
  "projectType": "application",
  "root": "apps/server",
  "sourceRoot": "apps/server",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o ../../dist/apps/server/main main.go",
        "cwd": "apps/server"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run main.go",
        "cwd": "apps/server"
      }
    }
  },
  "tags": []
}